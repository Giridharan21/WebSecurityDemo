@model WebSecurityDemo.Models.LoginInfo
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
</head>
<body>
    @using (Html.BeginForm("Login", "Login", FormMethod.Post)) {
        <span>Username: </span>@Html.TextBoxFor(m=>m.UN) <br />
        <span>Password: </span>@Html.PasswordFor(m=>m.PW)<br />
        <input type="submit" value="Log In" />
    }
</body>
</html>
